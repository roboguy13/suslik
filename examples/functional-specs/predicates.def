predicate fold_with(loc x, loc y, int curr, set s, pred p) {
| x == null => { y == null ; emp}
| not (x == null) => {
    s =i {curr} ++ s1 &&
    p(old, curr)
      ;
    [x, 2] **
    x :-> old **
    (x+1) :-> xNxt **
    [y, 2] **
    y :-> curr **
    (y+1) :-> yNxt **
    fold_with(xNxt, yNxt, curr, s1, p)
  }
}

