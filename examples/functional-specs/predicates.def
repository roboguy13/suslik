predicate height_bin_tree(loc x, set s, int height) {
| x == 0 => { height == 0 && s =i {} ; emp }
| not (x == 0) => {
    height == 1 + (heightL <= heightR ? heightR : heightL)
    && s =i {v} ++ sL ++ sR
      ;
    [x, 3] **
    x :-> v **
    (x+1) :-> left **
    (x+2) :-> right **
    height_bin_tree(left, sL, heightL) **
    height_bin_tree(right, sR, heightR)
  }
}

